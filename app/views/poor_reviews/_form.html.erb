<%= simple_form_for(@poor_review) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :user_id %>
    <%= f.input :invite_id %>
    <%= f.input :first_name %>
    <%= f.input :last_name %>
    <%= f.input :message %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
