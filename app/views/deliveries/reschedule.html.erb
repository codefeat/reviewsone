<h1>Listing Days</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @deliveries.each do |delivery| %>
      <tr>
        <td><%= day.date %></td>
        <td><%= link_to 'Edit', edit_delivery_path(delivery) %></td>
      </tr>
    <% end %>
  </tbody>
</table>